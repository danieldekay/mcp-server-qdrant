{
    "servers": {
        "qdrant-stdio": {
            "type": "stdio",
            "command": "uv",
            "args": [
                "run",
                "mcp-server-qdrant"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "QDRANT_URL": ":memory:",
                "COLLECTION_NAME": "demo",
                "EMBEDDING_PROVIDER": "fastembed",
                "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
            }
        },
    }
}